{
    "presets": ["@babel/preset-env", "@babel/preset-react", "@babel/preset-typescript"],
    "plugins": [
        ["module-resolver", {
            "root": ["./src"],
            "alias": {
                "api": "./src/api",
                "components": "./src/components",
                "context": "./src/context",
                "pages": "./src/pages",
                "providers": "./src/providers",
                "types": "./src/types",
                "utils": "./src/utils"
            }
        }]
    ]
}
